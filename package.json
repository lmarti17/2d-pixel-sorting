{
  "name": "parcel-plate",
  "version": "1.0.0",
  "description": "Parcel starter including scss and hmr",
  "scripts": {
    "dev": "parcel ./src/index.html --port 3000",
    "clear": "rm -rf ./dist node_modules/.cache .cache",
    "build": "rm -rf ./dist ./cache && parcel build src/index.html  --public-url ./dist/ --no-source-maps",
    "build-vercel": "rm -rf ./dist ./cache && parcel build src/index.html --no-source-maps"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "author": "Lucas Martin <contact@lucasmartin.fr>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "parcel": "^2.0.0-rc.0",
    "posthtml-modules": "^0.7.4"
  }
}
